                            </div>
                            <!-- end widget edit box -->
                            <!-- widget content -->
                            <div class="widget-body">
                                @*<div id="cncLoader"></div>*@
                                <div id="cncLoaderMessage">
                                </div>
                                <div id="showMessage"> </div>
                                @using (Html.BeginForm("CardIssuerResponseForCardRequests", "Card", FormMethod.Post, new { @class = "smart-form", role = "form", name = "SearchCardRequests" }))
                                {
                                    @Html.AntiForgeryToken()
                                    @Html.ValidationSummary("", new { @class = "text-danger" })
