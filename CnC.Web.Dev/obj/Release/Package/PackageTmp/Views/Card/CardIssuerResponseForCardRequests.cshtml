        function PostCardInfo(rqFrom, obp) {
            var cardNumber = document.getElementById("txtCardNo").value;
            //var clientId = document.getElementById("txtClientId").value;
            var cardTitle = document.getElementById("txtCardTitle").value;
            var hv = $('#hfFormId').val();
            var cardExpiry = document.getElementById("txtExpiryDate").value;
            var customerClientId = document.getElementById("txtClientId").value;
            var cardId = document.getElementById("txtCardId").value;
            if (rqFrom == "f") {
                if (hv != "") {
                    $("#cncLoader").show();
                    var failureReason = $("#txtFailure").val();
                    CheckIsValid();
                    $.ajax({
                        url: "/Card/UpdateCardStatusFailed",
                        contentType: "application/json; charset=utf-8",
                        data: { key: hv, failureReason: failureReason },
                        dataType: "json",
                        success: function (data) {
                            $("#cncLoader").hide();
                            if (data[0] == "200") {
                                $(obp).dialog("close");
                                $("#cncLoaderMessage").append(data[1]);
                                $("#cncLoaderMessage").show();
                            } else {
                                $(obp).dialog("close");
                                $("#showMessage").removeClass("alert alert-danger fade in");
                                $("#showMessage").empty();
                                $("#showMessage").show();
                                $("#showMessage").addClass("alert alert-danger fade in");
                                $("#showMessage").append("<button class='close' onclick='UpdatePanelDiv();'>×</button> <i class='fa-fw fa fa-times'></i><strong>'@localizationService.GetResource("Cnc.Error",null,"Error")'!</strong> '" + data[1] + "'");
                            }
                        },
                        error: function () {
                            $("#cncLoader").hide();
                            $(obp).dialog("close");
                            $("#showMessage").removeClass("alert alert-danger fade in");
                            $("#showMessage").empty();
                            $("#showMessage").show();
                            $("#showMessage").addClass("alert alert-danger fade in");
                            $("#showMessage").append("<button class='close' onclick='UpdatePanelDiv();'>×</button> <i class='fa-fw fa fa-times'></i><strong>" + '@localizationService.GetResource("Cnc.Error",null,"Error")' + "!" + "</strong> '" + '@localizationService.GetResource("Cnc.RequiredField",null,"The Field is Required")' + "'");
                        }
                    });
                } else {
                    alert("Missing required values.");
                }
            } else {
                if (cardNumber != "" &&
                    cardTitle != "" &&
                    cardExpiry != "" &&
                    customerClientId != "" &&
                    hv != "" &&
                    customerClientId != "" &&
                    cardId != "") {
                    $("#cncLoader").show();
                    CheckIsValid();
                    $.ajax({
                        url: "/Card/UpdateCardStatus",
                        contentType: "application/json; charset=utf-8",
                        data: {
                            key: hv,
                            cardNumber: cardNumber,
                            cardTitle: cardTitle,
                            cardExpiry: cardExpiry,
                            cardId: cardId,
                            customerClientId: customerClientId
                        },
                        dataType: "json",
                        success: function (data) {
                            $("#cncLoader").hide();
                            if (data[0] == "200") {
                                ClearForm();
                                $(obp).dialog("close");
                                $("#cncLoaderMessage").append(data[1]);
                                $("#cncLoaderMessage").show();
                            } else {
                                $(obp).dialog("close");
                                $("#showMessage").removeClass("alert alert-danger fade in");
                                $("#showMessage").empty();
                                $("#showMessage").show();
                                $("#showMessage").addClass("alert alert-danger fade in");
                                $("#showMessage").append("<button class='close' onclick='UpdatePanelDiv();'>×</button> <i class='fa-fw fa fa-times'></i><strong>'@localizationService.GetResource("Cnc.Error",null,"Error")'!</strong> '" + data[1] + "'");
                            }
                            //if (results.includes("successfully")) {

                            //    //window.location.reload();
                            //}
                        },
                        error: function () {
                            $(obp).dialog("close");
                            $("#cncLoader").hide();
                            $("#showMessage").removeClass("alert alert-danger fade in");
                            $("#showMessage").empty();
                            $("#showMessage").show();
                            $("#showMessage").addClass("alert alert-danger fade in");
                            $("#showMessage").append("<button class='close' onclick='UpdatePanelDiv();'>×</button> <i class='fa-fw fa fa-times'></i><strong>'@localizationService.GetResource("Cnc.Error",null,"Error")'!</strong><strong>'@localizationService.GetResource("Cnc.Fail",null,"Fail")'!</strong> An error occured while updating. Please try again. ");
                        }
                    });
                } else {
                    $(obp).dialog("close");
                    $("#cncLoader").hide();
                    $("#showMessage").removeClass("alert alert-danger fade in");
                    $("#showMessage").empty();
                    $("#showMessage").show();
                    $("#showMessage").addClass("alert alert-danger fade in");
                    $("#showMessage").append("<button class='close' onclick='UpdatePanelDiv();'>×</button> <i class='fa-fw fa fa-times'></i><strong>'@localizationService.GetResource("Cnc.Error",null,"Error")'!</strong><strong>'@localizationService.GetResource("Cnc.Fail",null,"Fail")'!</strong> Missing required values. Please try again. ");
                }
            }
        }
