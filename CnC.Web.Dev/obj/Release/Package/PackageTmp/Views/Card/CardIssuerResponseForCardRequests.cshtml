                                            <label class="">
                                                @Html.CheckBox("ShowFailedPayments", isShowFailedPayments, new { value = "1" })
                                                Show Declined
                                            </label>
                                        </div>
                                        <div class="row">
                                            @{Html.RenderPartial("_SearchButtons");}
                                        </div>
                                    </fieldset>
                                                }
                            </div>
                            <!-- end widget content -->
                            <div class="Container Flipped">
                                <div class="Content">
                                    <div class="table-responsive" style="padding-bottom:50px;">
                                        @{
                                            if (Model != null && Model.Count > 0)
                                            {
                                                <table class="table table-bordered table-striped">
                                                    <thead>
                                                        <tr>
                                                            <th>@localizationService.GetResource("Cnc.Request#", null, "Request #")</th>
                                                            <th>@localizationService.GetResource("Cnc.Name", null, "Name")</th>
                                                            <th>@localizationService.GetResource("Cnc.NationalID", null, "National ID #")</th>
                                                            <th>@localizationService.GetResource("Cnc.RequestDate", null, "Request Date")</th>
                                                            <th title="Method of Payment">@localizationService.GetResource("Cnc.PaymentMode", null, "Payment Mode")</th>
                                                            <th>@localizationService.GetResource("Cards", null, "Cards")</th>
                                                            <th>@localizationService.GetResource("Cnc.DeclinedReason", null, "Declined Reason")</th>
                                                            <th></th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        @foreach (var cardRequestForm in Model.Where(x => x.CardRequests.Count > 0).ToList())
                                                        {
                                                            string isRequired = "false";

                                                            string paymentDesc = string.Empty;
                                                            int i = 0;
                                                            foreach (var payment in cardRequestForm.Payments)
                                                            {
                                                                paymentDesc = paymentDesc + "" +
                                                                                (payment.PaymentMethodId == (int)PaymentMethod.RAHYAB
                                                                                || payment.PaymentMethodId == (int)PaymentMethod.Admin
                                                                                ? "Manual" : ((PaymentMethod)payment.PaymentMethodId).ToString())
                                                                                + ",";
                                                                paymentDesc = paymentDesc.Trim(',');
                                                            }
                                                            <text>
                                                                @if (i == 0)
                                                                {
                                                                    i = 1;
                                                                    <text>
                                                                        <tr>
                                                                            <td>@cardRequestForm.Id</td>
                                                                            <td>@cardRequestForm.Customer.User.FullName</td>
                                                                            <td>@cardRequestForm.Customer.NIC</td>
                                                                            <td>@cardRequestForm.CreatedOn.ToString("yyyy/MM/dd HH:mm")</td>
                                                                            <td>@paymentDesc</td>
                                                                            @{
                                                                                string cardDesc = string.Empty;
                                                                                string fmCrId = string.Empty;
                                                                                cardDesc = cardRequestForm.CardRequests[0].CardType.Name;
                                                                                var cardRequestStatus = cardRequestForm.CardRequests[0].StatusString;
                                                                                fmCrId = cardRequestForm.Id + "-" + cardRequestForm.CardRequests[0].Id /*+ "#" + cardRequestForm.Customer.CardServiceProviderClientId*/;
                                                                            }
                                                                            <td>@cardDesc</td>
                                                                            <td class="wraptext">@cardRequestForm.CardRequests[0].DeclinedReason</td>
                                                                            @if (!cardRequestForm.CardRequests[0].CardType.IsProofOfSourceOfFundsRequired)
                                                                            {
                                                                                isRequired = "false";
                                                                            }
                                                                            @if (cardRequestForm.CardRequests[0].CardType.IsProofOfSourceOfFundsRequired)
                                                                            {
                                                                                isRequired = "true";
                                                                            }
                                                                            <td>
                                                                                <a onclick='OpenDialog("@fmCrId","@cardRequestForm.Customer.NIC","@isRequired");' style="cursor:pointer" id="dialog_link">@localizationService.GetResource("Cnc.Approved", null, "Approved")</a><span style="margin-left: 5px; margin-right: 5px;">|</span>
                                                                                <a onclick='UpdateCard("@fmCrId");' style="cursor:pointer" id="card_failed">@localizationService.GetResource("Cnc.Declined", null, "Declined")</a>
                                                                            </td>
                                                                        </tr>
                                                                    </text>
                                                                                    cardRequestForm.CardRequests.RemoveAt(0);
                                                                                }
                                                                @foreach (var cardRequest in cardRequestForm.CardRequests)
                                                                {
                                                                    <text>
                                                                        <tr>
                                                                            <td></td>
                                                                            <td></td>
                                                                            <td></td>
                                                                            <td></td>
                                                                            <td></td>
                                                                            <td class="CardType">@cardRequest.CardType.Name</td>
                                                                            <td class="wraptext">
                                                                                @cardRequest.DeclinedReason
                                                                                @*@cardRequest.StatusString*@
                                                                            </td>
                                                                            <td>
                                                                                @{
                                                                                    string tempVal = cardRequestForm.Id + "-" + cardRequest.Id /*+ "#" + cardRequestForm.Customer.CardServiceProviderClientId;*/ ;
                                                                                }
                                                                                @if (cardRequest.CardType.IsProofOfSourceOfFundsRequired)
                                                                                {
                                                                                    isRequired = "true";
                                                                                }
                                                                                @if (!cardRequest.CardType.IsProofOfSourceOfFundsRequired)
                                                                                {
                                                                                    isRequired = "false";
                                                                                }
                                                                                <a onclick='OpenDialog("@tempVal","@cardRequestForm.Customer.NIC","@isRequired");' style="cursor:pointer" id="dialog_link">@localizationService.GetResource("Cnc.Approved", null, "Approved")</a><span style="margin-left: 5px; margin-right: 5px;">|</span>
                                                                                <a onclick='UpdateCard("@tempVal");' style="cursor:pointer" id="card_failed">@localizationService.GetResource("Cnc.Declined", null, "Declined")</a>

                                                                            </td>
                                                                        </tr>
                                                                    </text>
                                                                                    }
                                                            </text>
                                                                                    }
                                                        <input type="text" hidden id="txtHiddenCustomerSourceFunds1" />
                                                        <input type="text" hidden id="txtHiddenCustomerSourceFunds2" />
                                                        <input type="text" hidden id="txtHiddenCustomerSourceFunds3" />
                                                        <input type="text" hidden id="txtHiddenCustomerSourceFunds4" />

                                                        <input type="text" hidden id="txtHiddenCustomerAddressDocs1" />
                                                        <input type="text" hidden id="txtHiddenCustomerAddressDocs2" />
                                                        <input type="text" hidden id="txtHiddenCustomerAddressDocs3" />
                                                        <input type="text" hidden id="txtHiddenCustomerAddressDocs4" />
                                                    </tbody>
                                                </table>
                                                                                        using (Html.BeginForm("CardIssuerResponseForCardRequests", "Card", FormMethod.Post, new { @class = "smart-form", role = "form", name = "SearchCardRequests" }))
                                                                                        {
                                                                                            {
                                                                                                Html.RenderPartial("_Pagination");
                                                                                            }
                                                                                        }
                                                                                    }
                                                                                    else
                                                                                    {
                                                                                        <div class="alert alert-info fade in" style="margin-top:5px;">
                                                                                            <button class="close" data-dismiss="alert">×</button>
                                                                                            <i class="fa-fw fa fa-cube"></i>
                                                                                            <strong>@localizationService.GetResource("Cnc.Sorry", null, "Sorry")! </strong>@localizationService.GetResource("Cnc.NoRecord", null, "No Records Found")
                                                                                        </div>
                                                                                    }
                                        }
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- end widget div -->
                    </div>
                    <!-- end widget -->
                </article>
                <!-- WIDGET END -->
            </div>
            <!-- end row -->
        </section>
        <!-- end widget grid -->
    </div>
    <!-- end widget content -->
</div>
