@section pagespecific {
    <script>

        var a1 = false;
        var a2 = false;
        var a3 = false;
        var a4 = false;

        var s1 = false;
        var s2 = false;
        var s3 = false;
        var s4 = false;

        var addressDoc1 = "";
        var addressDoc2 = "";
        var addressDoc3 = "";
        var addressDoc4 = "";

        var sourceFunds1 = "";
        var sourceFunds2 = "";
        var sourceFunds3 = "";
        var sourceFunds4 = "";

        function textAreaAdjust(o) {
            o.style.height = "1px";
            o.style.height = (25 + o.scrollHeight) + "px";
        }
        $(document).ready(function () {
            $('#dialog_Image').dialog({
                autoOpen: false,
                width: 700,
                resizable: false,
                modal: true,
                title: "Document",

            });

            $("#cncLoader").hide();
            $("#cncLoaderMessage").hide();
            $('form').on('reset', function () {
                var $form = $(this);
                $form.find('.error,.valid').css('border-color', '').removeClass('error').removeClass('valid');
                $form.find('.form-error').remove();
            });
        });

        $("#txtFailure").on("keypress", function (e) {
            $("#lblShowErrorMessage").hide();
        })

        $('#dialog_simple').dialog({
            autoOpen: false,
            width: 600,
            resizable: false,
            modal: true,
            title: "Card",
            buttons: [
                {
                    html: "<i class='fa fa-trash-o'></i>&nbsp; @localizationService.GetResource("Cnc.SaveChanges", null,"Save Changes")",
                    "class": "btn btn-success",
                    click: function () {
                        //alert("Save Changes");
                        var form = $("#card-info-form");
                        //alert(form);
                        form.validate();
                        if (!form.valid()) {
                            return false;
                        };
                        $(".btn-default").attr("disabled", true);
                        var isTrue = ValidateImages();
                        if (isTrue == true) {
                            PostCardInfo("s", this);
                            ClearForm();
                        }
                    }
                }, {
                    html: "<i class='fa fa-times'></i>&nbsp; @localizationService.GetResource("Cnc.Cancel", null, "Cancel")",
                    "class": "btn btn-default",
                    click: function () {
                        $(".btn-success").attr("disabled", true);
                        ClearForm();
                        var validator = $("#card-info-form").validate();
                        validator.resetForm();
                        $("#card-info-form").find('.state-error,.input,.smart-form').removeClass('state-error');
                        $(this).dialog("close");
                    }
                }
            ]
        });
        function ValidateImages() {
            //if (a1 == false || a2 == false || a3 == false || a4 == false || s1 == false || s2 == false || s3 == false
            //    || s4 == false) {
            //    document.getElementById("forcustomerProofofAddress").style.display = "";
            //    return false;
            //}

            if ((addressDoc1 != "" && a1 == false) || (addressDoc2 != "" && a2 == false)
                || (addressDoc3 != "" && a3 == false) || (addressDoc4 != "" && a4 == false)
                || (sourceFunds1 != "" && s1 == false) || (sourceFunds2 != "" && s2 == false)
                || (sourceFunds3 != "" && s3 == false) || (sourceFunds4 != "" && s4 == false)) {
                document.getElementById("forcustomerProofofAddress").style.display = "";
                return false;
            }

            return true;
        }
        $('#dialog_card_failed').dialog({
            autoOpen: false,
            width: 400,
            resizable: false,
            modal: true,
            title: '@localizationService.GetResource("Cnc.CardInfo",null,"CardInfo")',
            buttons: [
                {
                    html: "<i class='fa fa-trash-o'></i>&nbsp; @localizationService.GetResource("Cnc.Decline", null, "Decline")",
                    "class": "btn btn-danger",
                    click: function () {
                        if ($("#txtFailure").val() == "") {
                            //
                            $("#lblShowErrorMessage").show();
                            return false
                        }
                        else {
                            $("#lblShowErrorMessage").hide();
                        }
                        //var form = $("#card-info-form123");

                        //form.validate();
                        //if (!form.valid()) {
                        //    return false;
                        //};
                        $(".btn-default").attr("disabled", true);
                        var form = $("#card-info-form");
                        form.validate();
                        if (!form.valid()) {
                            return false;
                        };
                        PostCardInfo("f", this);
                    }
                }, {
                    html: "<i class='fa fa-times'></i>&nbsp; @localizationService.GetResource("Cnc.Cancel", null, "Cancel")",
                    "class": "btn btn-default",
                    click: function () {
                        $(".btn-danger").attr("disabled", true);
                        ClearForm();
                        var validator = $("#card-info-form123").validate();
                        validator.resetForm();
                        $("#card-info-form123").find('.state-error,.input,.smart-form').removeClass('state-error');
                        $(this).dialog("close");
                    }
                }
            ]
